# --- Core Runtime ---
RPC_URL=https://<your-rpc-endpoint>
DRY_RUN=true

# --- Base asset config (SOL pools by default) ---
BASE_MINT=So11111111111111111111111111111111111111112
BASE_DECIMALS=9
BASE_SYMBOL=SOL
BASE_SYMBOL_POSITION=suffix
BASE_DISPLAY_DECIMALS=5
BUY_AMOUNT_BASE=0.25
MIN_NET_PROFIT_BASE=0.01
NEAR_MISS_DELTA=0.0025
SLIPPAGE_BPS=50
LAMPORTS_PRICE_IN_BASE=1

# --- DEX filtering ---
INCLUDE_DEXES=raydium,orca,meteora
INCLUDE_MODE=any
EXCLUDE_DEXES=tessera,solfi

# --- Parallelism & pacing ---
SCAN_INTERVAL_MS=250
PER_TOKEN_COOLDOWN_MS=0
MAX_PARALLEL=4

# --- HTTP ---
HTTP_TIMEOUT_MS=15000
FETCH_RETRIES=5
RETRY_BACKOFF_MS=500

# --- Jupiter ---
JUP_MODE=FREE
JUP_API_KEY=

# --- Priority fees & Jito ---
PRIORITY_LAMPORTS=7500
JITO_TIP_LAMPORTS=2500
JITO_MODE=off
JITO_APPLY_MODE=both
JITO_TIP_SOL_BUY=0.006
JITO_TIP_SOL_SELL=0.008

# --- Helius Sender ---
SENDER_ENABLED=false
SENDER_ENDPOINT=
SENDER_API_KEY=
SENDER_SKIP_PREFLIGHT=true
SENDER_MAX_RETRIES=0
SENDER_CONFIRM_COMMITMENT=confirmed

# --- Wallet (unused in DRY_RUN) ---
PRIVATE_KEY_B58=

